---
const { educationByYear } = Astro.props;
---

<li>
  <section class="mb-4">
    <h3 class="text-4xl">
      {educationByYear.year}
    </h3>
    <ul class="list-outside">
      {
        educationByYear.educations.map((education) => (
          <li>
            <span class="text-base text-black text-opacity-90">
              {education.title}
            </span>
          </li>
        ))
      }
    </ul>
  </section>
</li>
