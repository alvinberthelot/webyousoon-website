---
import Event from "./event.astro";
const { eventsByYear } = Astro.props;
---

<li>
  <section class="mb-4">
    <h3 class="text-opacity-20">
      {eventsByYear.year}
    </h3>
    <ul class="list-outside list-square">
      {eventsByYear.events?.map((event) => <Event event={event} />)}
    </ul>
  </section>
</li>
