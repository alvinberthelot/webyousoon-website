<html lang="fr">
  <head>
    <meta charset="UTF-8">
    <title>{{ website.title }}</title>
    <link rel="canonical" href="{{ website.company.website.completeUrl }}"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="generator" content="Eleventy">
    <!-- metas classiques -->
    <meta name="description" content="{{ website.company.description }}"/>
    <meta name="keywords" content="{{ website.company.keywords | join(',') }}"/>
    <!-- metas Open Graph -->
    <meta property="og:locale" content="fr_FR"/>
    <meta property="og:type" content="website"/>
    <meta property="og:title" content="{{ website.title }}"/>
    <meta property="og:description" content="{{ website.company.description }}"/>
    <meta property="og:url" content="{{ website.company.website.completeUrl }}"/>
    <meta property="og:site_name" content="{{ website.title }}"/>

