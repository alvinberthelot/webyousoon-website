{
  "name": "webyousoon-website",
  "version": "1.0.0",
  "author": "Alvin BERTHELOT",
  "description": "Static generator of Webyousoon's website",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env ELEVENTY_ENV=dev eleventy --serve --input=./src --output=./dist",
    "build": "cross-env ELEVENTY_ENV=prod eleventy --input=./src --output=./dist",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvinberthelot/webyousoon-website.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alvinberthelot/webyousoon-website/issues"
  },
  "homepage": "https://github.com/alvinberthelot/webyousoon-website#readme",
  "dependencies": {
    "@11ty/eleventy": "0.10.0",
    "html-minifier": "4.0.0"
  },
  "devDependencies": {
    "cross-env": "7.0.2"
  }
}
